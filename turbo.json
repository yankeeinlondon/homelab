{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "watch": {
        "cache": false,
        "persistent": true
    },
    "lint": {},
    "test": {},
    "upgrade":{
        "dependsOn": ["^upgrade"],
        "outputs": ["./"]
    }
  }
}
